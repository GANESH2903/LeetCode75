class Solution {
    public boolean isSubsequence(String s, String t) {
        short indi = -1;
        short count = 0;
        for(int i = 0; i < s.length(); i++){
            if(indi < t.indexOf(s.charAt(i))){
                indi = (short) t.indexOf(s.charAt(i));
                count++;
            }
        }
        if(count == s.length())
            return true;
        else
            return false;
    }
}
